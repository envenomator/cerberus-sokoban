                             57         ;; Optimise for the case when this side has 8 bits of data or
                             58         ;; less.  This is often the case with support address calls.
   000B 20 04         [12]   59         jr      NZ,2$
   000D 06 08         [ 7]   60         ld      b,#8
   000F 79            [ 4]   61         ld      a,c
   0010                      62 1$:
                             63         ;; Taken from z88dk, which originally borrowed from the
