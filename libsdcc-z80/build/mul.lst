                             29 .area   _CODE
                             30 
                             31 .globl	__mulint
                             32 
   0000                      33 __mulint:
   0000 F1            [10]   34         pop     af
   0001 C1            [10]   35         pop     bc
   0002 D1            [10]   36         pop     de
   0003 D5            [11]   37         push    de
   0004 C5            [11]   38         push    bc
   0005 F5            [11]   39         push    af
                             40 
                             41 	;; 16-bit multiplication
                             42 	;;
                             43 	;; Entry conditions
                             44 	;; bc = multiplicand
                             45 	;; de = multiplier
                             46 	;;
                             47 	;; Exit conditions
                             48 	;; hl = less significant word of product
                             49 	;;
                             50 	;; Register used: AF,BC,DE,HL
   0006                      51 __mul16::
   0006 AF            [ 4]   52 	xor	a,a
   0007 6F            [ 4]   53 	ld	l,a
   0008 B0            [ 4]   54 	or	a,b
   0009 06 10         [ 7]   55 	ld	b,#16
