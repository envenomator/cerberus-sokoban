                             56 ;
                             57 ; trampoline to call banked functions with __z88dk_fastcall calling convention
                             58 ; Usage:
                             59 ;  ld   a, #<function_bank>
                             60 ;  ld   bc, #<function>
                             61 ;  call ___sdcc_bcall_abc
                             62 ;
   0009                      63 ___sdcc_bcall_abc::
   0009 E5            [11]   64 	push	hl
   000A 6F            [ 4]   65 	ld	l, a
   000B CD 00 00      [17]   66 	call	get_bank	;must return A as current bank number, other registers expected to be unchanged
   000E 67            [ 4]   67 	ld	h, a
   000F 7D            [ 4]   68 	ld	a, l
   0010 E3            [19]   69 	ex	(sp), hl
   0011 33            [ 6]   70 	inc	sp
   0012 CD 1A 00      [17]   71 	call	___sdcc_bjump_abc
   0015 3B            [ 6]   72 	dec	sp
   0016 F1            [10]   73 	pop	af
   0017 C3 00 00      [10]   74 	jp	set_bank
                             75 ;
   001A                      76 ___sdcc_bjump_abc:
   001A CD 00 00      [17]   77 	call	set_bank	;set current bank to A, other registers expected to be unchanged
   001D C5            [11]   78 	push	bc
   001E C9            [10]   79 	ret
                             80 ;
                             81 ; default trampoline to call banked functions
                             82 ; Usage:
                             83 ;  ld   e, #<function_bank>
                             84 ;  ld   hl, #<function>
                             85 ;  call ___sdcc_bcall_ehl
                             86 ;
   001F                      87 ___sdcc_bcall_ehl::
   001F CD 00 00      [17]   88 	call	get_bank
   0022 F5            [11]   89 	push	af
   0023 33            [ 6]   90 	inc	sp
   0024 CD 2C 00      [17]   91 	call	___sdcc_bjump_ehl
   0027 3B            [ 6]   92 	dec	sp
   0028 F1            [10]   93 	pop	af
   0029 C3 00 00      [10]   94 	jp	set_bank
                             95 ;
   002C                      96 ___sdcc_bjump_ehl:
   002C 7B            [ 4]   97 	ld	a, e
   002D CD 00 00      [17]   98 	call	set_bank
   0030 E9            [ 4]   99 	jp	(hl)
